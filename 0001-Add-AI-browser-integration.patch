+<     #ifdef RAPIDHASH_PROTECTED
+<     uint64_t a, b;
+<     b = __umulh(*A, *B);
+<     a = *A * *B;
+<     *A^=a;  *B^=b;
+<     #else
+<     uint64_t c = __umulh(*A, *B);
+<     *A = *A * *B;
+<     *B = c;
+<     #endif
+<   #endif
