+<   #elif defined(__BIG_ENDIAN__) || (defined(__BYTE_ORDER__) && __BYTE_ORDER__ == __ORDER_BIG_ENDIAN__)
+<     #define RAPIDHASH_BIG_ENDIAN
+<   #else
