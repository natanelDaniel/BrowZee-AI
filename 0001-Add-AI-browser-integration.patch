+-    CHUNK(1, 1); CHUNK(k0, 0);
+-    CHUNK(1, 1); CHUNK(k0, 0);
+-    CHUNK(1, 1); CHUNK(k0, 0);
+-  } while (--iters > 0);
+-
+-  while (len >= 40) {
+-    CHUNK(k0, 0);
+-    len -= 40;
+-  }
+-  if (len > 0) {
+-    s = s + len - 40;
+-    CHUNK(k0, 0);
+-  }
+-  j += i << 32;
+-  a = HashLen16(a, j);
+-  h += g << 32;
+-  b += h;
+-  c = HashLen16(c, f) + i;
+-  d = HashLen16(d, e + result[0]);
+-  j += e;
+-  i += HashLen16(h, t);
+-  e = HashLen16(a, d) + j;
+-  f = HashLen16(b, c) + a;
+-  g = HashLen16(j, i) + c;
+-  result[0] = e + f + g + h;
+-  a = ShiftMix((a + g) * k0) * k0 + b;
+-  result[1] += a + result[0];
+-  a = ShiftMix(a * k0) * k0 + c;
